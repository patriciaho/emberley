<div class="new_image">
	<h1>Images New</h1>
	<%= form_for @image, :html => {:multipart => true} do |f| %>
	  <%= f.text_field :name, placeholder: "image name", class: "form-control" %>
	  <%= f.select :category, ['cars', 'animals', 'people'], class: "selectpicker" %>
	  <%= f.file_field :image, class: "form-control" %>
	  <%= f.submit "Upload", class: "btn btn-primary" %>
	<% end %>
</div>